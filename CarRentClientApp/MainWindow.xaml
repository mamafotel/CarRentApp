<Window x:Class="CarRentClientApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarRentClientApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button x:Name="List" VerticalAlignment="Top" HorizontalAlignment="Left" FontSize="16" Content="Button" Click="List_Click"></Button>
            <ListBox x:Name="carListBox" Width="500" Height="400" Margin="0 20 0 0">
                <ListBox.ItemTemplate>
                    <DataTemplate>
                        <StackPanel>
                            
                            <TextBlock Text="{Binding Brand}" />
                            <TextBlock Text="{Binding Model}" />
                            <TextBlock Name="Category" Text="{Binding CategoryId}" />
                        </StackPanel>
                    </DataTemplate>
                </ListBox.ItemTemplate>
            </ListBox>
        </StackPanel>
    </Grid>
</Window>
